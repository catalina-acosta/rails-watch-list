<h1><%= @list.name %></h1>
<ul>
    <% @list.bookmarks.each do |bookmark| %>
      <li><%= bookmark.movie.title %>
      <br>
      <%= bookmark.comment %>
      <%= link_to "Delete",
        bookmark_path(bookmark),
        data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
      %>
      </li>
    <% end %>
  </ul>
<%#= create new bookmark -> redirect to bookmark-new %>
<p><%= link_to "Add bookmark", new_list_bookmark_path(@list) %></p>
